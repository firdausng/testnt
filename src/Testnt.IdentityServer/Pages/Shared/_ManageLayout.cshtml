    @RenderBody()

    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
    <script>
        var navbar = new Vue({
            el: '#navbar-vue',
            data: {
                navBarActive: false
            },
            methods: {
                onNavBarClick: function () {
                    this.navBarActive = !this.navBarActive;
                }
            }
        })
    </script>
    @RenderSection("VueScript", required: false)
